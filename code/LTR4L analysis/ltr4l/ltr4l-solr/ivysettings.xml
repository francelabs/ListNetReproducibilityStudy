<ivysettings>
    <settings defaultResolver="chain"/>
    <resolvers>
        <chain name="chain">
            <filesystem name="local"/>
            <ibiblio name="public" m2compatible="true" root="https://repo1.maven.org/maven2/"/>
            <ibiblio name="central" m2compatible="true" root="https://mvnrepository.com/"/>
            <ibiblio name="restlet" m2compatible="true" root="http://maven.restlet.com"/>
        </chain>
    </resolvers>
    <modules>
        <module organization="org.restlet.jee" resolver="restlet"/>
        <module organization="restlet" resolver="restlet"/>
        <module organization="org.restlet" resolver="restlet"/>
    </modules>
</ivysettings>